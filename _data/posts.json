---
layout: none
---

[
  {% for post in site.posts %}
  {
    "title": "{{ post.title | replace: '\', '' | replace: '"', '\"' }}",
    "content": "{{ post.content | strip_html | replace: '\', '' | replace: '"', '\"' | replace: '
', ' ' }}",
    "url": "{{ site.baseurl }}{{ post.url }}",
    "date": "{{ post.date }}"
  }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]
